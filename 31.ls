property psndname, psndchlnumon beginSprite me  global gprevqpnum, gcurrentspnum  set gprevqpnum to 0  puppetSound(psndchlnum, psndname)  set gcurrentspnum to the currentSpriteNumendon getBehaviorDescription  return "QP í âÇ…çáÇÌÇþÇƒÅAéüÅXÇ…ÉLÉÉÉXÉgÇéÊÇËë÷Ç¶ÇÈ"endon getPropertyDescriptionList  set description to [:]  set description to [#psndname: [#default: EMPTY, #format: #string, #comment: "çƒêðÇÝÇÈì‡ïîÉLÉÉÉXÉgñº"], #psndchlnum: [#default: 1, #format: #integer, #comment: "âþêŠçƒêðÉTÉEÉìÉhÉ`ÉÉÉlÉãî‘çÜ"]]  return descriptionendon exitFrame me  global gprevqpnum, gcurrentspnum  set nowqpnum to IsPastCuePoint(sound(psndchlnum), "Q")  if nowqpnum > gprevqpnum then    set newcstnum to the castNum of sprite gcurrentspnum + 1    set the member of sprite gcurrentspnum to member newcstnum    updateStage()    set gprevqpnum to nowqpnum  else    if soundBusy(psndchlnum) = 0 then      puppetSprite(gcurrentspnum, 0)      go(the frame + 1)    end if  end if  go(the frame)end
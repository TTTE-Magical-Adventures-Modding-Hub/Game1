on exitFrame  global wheretogo, stage  if soundBusy(1) = 0 then    sendAllSprites(#lightoff)    cursor([the number of member "def", the number of member "def_mask"])    if wheretogo = "èoëË" then      set the member of sprite 40 to the number of member "seikai"      updateStage()      sndply2()    end if    puppetSprite(40, 0)    go(wheretogo)  end if  go(the frame)endon sndply2  global stage, qnum  repeat while soundBusy(1)  end repeat  if qnum = 2 then    set sndno to stage + 3  else    if stage > 5 then      set sndno to 8    else      set sndno to stage + 3    end if  end if  puppetSound(1, sndno)  repeat while soundBusy(1) = 1  end repeatend
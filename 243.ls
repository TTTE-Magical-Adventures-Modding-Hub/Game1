property sprnum, cursorname, cursormaskon beginSprite me  set sprnum to the currentSpriteNumendon getPropertyDescriptionList me  set dlist to [:]  set dlist to [#cursorname: [#comment: "ÔúÁX«›«»…L………X…gÒ∫", #format: #string, #default: EMPTY], #cursormask: [#comment: "…}…X…N…L………X…gÒ∫", #format: #string, #default: EMPTY]]  return dlistendon mouseWithin me  cursor([the member of member cursorname, the member of member cursormask])endon mouseLeave me  cursor(0)end
property sprnum, waittime, tmptimeon beginSprite me  set sprnum to the currentSpriteNum  set tmptime to the timer + waittimeendon getPropertyDescriptionList me  set descriptionlist to [:]  set descriptionlist to [#waittime: [#comment: "ë“Çøéûä‘", #format: #integer, #default: 60]]  return descriptionlistendon exitFrame me  if the timer > tmptime then    sendAllSprites(#next)  end ifend
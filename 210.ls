property sprnum, startsprite, soundspriteon beginSprite me  global wheretogo, clickstopflag, mdownspnum  set mdownspnum to 0  set sprnum to the currentSpriteNum  set wheretogo to "DUMMY"  set clickstopflag to "No"endon getPropertyDescriptionList me  set descriptionlist to [:]  set descriptionlist to [#startsprite: [#comment: "É{É^ÉìÇÃêÊì™ÉXÉvÉâÉCÉgî‘çÜ", #format: #integer, #default: the currentSpriteNum], #soundsprite: [#comment: "ââëtÉrÉwÉCÉrÉAÇÃÇ†ÇÈÉXÉvÉâÉCÉg", #format: #integer, #default: the currentSpriteNum]]  return descriptionlistendon mouseDown me  global debugmode, buttonlist, answer, answerloop, stage, wheretogo, clickstopflag, soundnumber, maxclicknum, qnum, mdownspnum  set mdownspnum to the currentSpriteNum  if wheretogo = "huseikai" then    repeat while soundBusy(1) = 1    end repeat    go("huseikai")    exit  else    if clickstopflag = "No" then      if soundBusy(1) = 1 then        puppetSound(1, 0)        sendAllSprites(#lightoff)      end if      set answer to sprnum - startsprite + 1      set sdnum to getAt(buttonlist, answer) + 1      set sdstr to changenumber2mml(sdnum)      sendSprite(soundsprite, #play, sdstr)      sendSprite(sdnum + startsprite - 1, #lighton)      set ans to answercheck()      if debugmode = "ON" then        set ans to 1      end if      if ans = 1 then        set answerloop to answerloop + 1        if qnum = 2 then          set loopnum to stage * 2          if loopnum > maxclicknum then            set loopnum to maxclicknum          end if        else          set loopnum to stage        end if        if answerloop > loopnum then          set stage to stage + 1          set clickstopflag to "Yes"          if answerloop > maxclicknum then            set wheretogo to "seikai"          else            set wheretogo to "èoëË"          end if        else          exit        end if      else        set wheretogo to "huseikai"      end if    end if    go("Jump")  end ifend
global levelon beginSprite  set sp to the currentSpriteNum  set myname to item 1 of the name of member the member of sprite sp  case myname of    "level":      set the member of sprite sp to "level," & level      puppetSound(1, "ask" & random(2) & ".aif")    "gameOver":    "clear":      puppetSound(1, "clear" & random(2) & ".aif")  end caseendon exitFrame  if soundBusy(1) then    go(the frame)  else    go(the frame + 1)  end ifend
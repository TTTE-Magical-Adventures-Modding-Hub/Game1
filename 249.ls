property sprnum, startMember, animenumber, loopon beginSprite me  set sprnum to the currentSpriteNum  set loop to 1endon getPropertyDescriptionList me  set descriptionlist to [:]  set descriptionlist to [#startMember: [#comment: "…A…j…≈…X…^≈[…g…L………X…gÒ∫", #format: #string, #default: EMPTY], #animenumber: [#comment: "…A…j…≈Ò·ÍÓ", #format: #integer, #default: 1]]  return descriptionlistendon exitFrame me  set the memberNum of sprite sprnum to the member of member (the number of member startMember + loop)  if loop = animenumber then    go(the frame + 1)    exit  end if  set loop to loop + 1end
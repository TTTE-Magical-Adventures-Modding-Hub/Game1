property sprnum, dispmember, rollflagon beginSprite me  set sprnum to the currentSpriteNum  set rollflag to 0endon getPropertyDescriptionList me  set descriptionlist to [:]  set descriptionlist to [#dispmember: [#comment: "Ô\È∂«›«»…X…v…‚…C…gÓëÁ‹", #format: #integer, #default: EMPTY]]  return descriptionlistendon getwaitflag me  return rollflagendon mouseWithin me  set the locH of sprite dispmember to 320  set rollflag to 1  sendAllSprites(#tagbehind)endon mouseLeave me  set the locH of sprite dispmember to -3000  set rollflag to 0  sendAllSprites(#returnobject)end
property sprnuma, soundsprite, startspriteon beginSprite me  set sprnum to the currentSpriteNum  set loop to 1endon getPropertyDescriptionList me  set descriptionlist to [:]  set descriptionlist to [#startsprite: [#comment: "…{…^…Ï«√Í Ïô…X…v…‚…C…gÓëÁ‹", #format: #integer, #default: the currentSpriteNum], #soundsprite: [#comment: "‚˛‰y‚‚Ît…r…w…C…r…A«√«Ü«»…X…v…‚…C…g", #format: #integer, #default: the currentSpriteNum]]  return descriptionlistendon question me  global musiclist, soundnumber, stage, loop  set ml to getAt(musiclist, soundnumber)  set snd to getsounddata(ml, loop)  set sndnum to changemml2number(snd)  sendSprite(soundsprite, #play, snd)  sendSprite(sndnum + startsprite - 1, #lighton)  case soundnumber of    1:      set spos to loop    2:      set spos to loop + 7    3:      set spos to loop + 17  end case  set onpcstname to "S" & spos  set onpspnum to 50 + spos  puppetSprite(onpspnum, 1)  set the member of sprite onpspnum to onpcstname  updateStage()  set loop to loop + 1  go(the frame + 1)end
property sprnum, waittime, tmptimeon beginSprite me  set sprnum to the currentSpriteNum  set tmptime to the timer + waittimeendon getPropertyDescriptionList me  set descriptionlist to [:]  set descriptionlist to [#waittime: [#comment: "ë“Çøéûä‘", #format: #integer, #default: 60]]  return descriptionlistendon exitFrame me  set flag to 0  repeat with i = 1 to 120    if the type of sprite i <> 0 then      set tmp to sendSprite(i, #getwaitflag)      if tmp = 1 then        set flag to 1      end if    end if  end repeat  if (the timer > tmptime) and (flag = 0) then    go(the frame + 1)    exit  end if  go(the frame)end
global level, soundnumber, stageon beginSprite  set level to soundnumber  set sp to the currentSpriteNum  set myname to item 1 of the name of member the member of sprite sp  case myname of    "level":      set the member of sprite sp to "level," & level      if soundnumber > 1 then        puppetSound(1, 13)      else        puppetSound(1, "start")      end if    "gameOver":      if stage > 3 then        puppetSound(1, 15)      else        puppetSound(1, 14)      end if    "clear":      puppetSound(1, 11)  end caseendon exitFrame  if soundBusy(1) then    go(the frame)  else    go(the frame + 1)  end ifend
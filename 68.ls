global room_flgon beginSprite me  global bqcount  set bqcount to 0  puppetSound(1, "ALL")endon exitFrame  global bqcount  if soundBusy(1) then    set qcount to IsPastCuePoint(sound(1), "Q")    if qcount > bqcount then      sendAllSprites(#lightoff)      set lonspnum to random(7) + 5      sendSprite(lonspnum, #lighton)      set lonspnum to lonspnum + random(7) + 1 + 5      sendSprite(lonspnum, #lighton)      set bqcount to qcount    end if    go(the frame)  else    movie_end_job()    cursor(200)    if room_flg = 0 then      go("start" & string(random(2)), the moviePath & "room")    else      go("gamereturn", the moviePath & "story" & room_flg & "_2")      set room_flg to 0    end if  end ifend
property sp, x, myname, now_sp_noglobal room_flgon mouseUpOutSide me  set now_sp_no to 0endon beginSprite me  set sp to the currentSpriteNum  set x to the locV of sprite sp  set myname to item 1 of the name of member the member of sprite the currentSpriteNumendon mouseDown me  set the locV of sprite sp to x + 1  set now_sp_no to sp  puppetSound(1, "click")  updateStage()endon mouseLeave me  set the locV of sprite sp to x  updateStage()endon mouseWithin me  if the mouseDown and (sp = now_sp_no) then    set the locV of sprite sp to x + 1    updateStage()  end ifendon mouseUp me  set the locV of sprite sp to x  updateStage()  if now_sp_no <> sp then    return   end if  set now_sp_no to 0  sendSprite(sp, #action)endon action  movie_end_job()  if room_flg = 1 then    set room_flg to 0    go("start" & string(random(2)), the moviePath & "room")  else  end ifend
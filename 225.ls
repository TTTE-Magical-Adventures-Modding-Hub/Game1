on exitFrame me  global stage, answer, musiclist, soundnumber  puppetSound(2, 0)  case soundnumber of    1:      set sndno to 10    2:      set sndno to 12    3:      set sndno to 12  end case  puppetSound(1, sndno)  repeat while soundBusy(1)  end repeat  set sndname to "level" & soundnumber  set bqcount to 0  set ml to getAt(musiclist, soundnumber)  puppetSound(1, sndname)  repeat while soundBusy(1)    set qcount to IsPastCuePoint(sound(1), "Q")    if qcount > bqcount then      sendAllSprites(#lightoff)      set snd to getsounddata(ml, qcount)      set sndnum to changemml2number(snd)      set lonspnum to sndnum + 5      sendSprite(lonspnum, #lighton)      set bqcount to qcount    end if  end repeat  if soundnumber = 3 then    go("clear")  else    set soundnumber to soundnumber + 1    set stage to 1    set answer to 0    go("ÉXÉeÅ[ÉWÉiÉìÉoÅ[")  end ifend
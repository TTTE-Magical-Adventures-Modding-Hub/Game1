property sprnum, animenum, animestart, animemaxon beginSprite me  set sprnum to the currentSpriteNum  set animenum to 0endon getPropertyDescriptionList me  set descriptionlist to [:]  set descriptionlist to [#animestart: [#comment: "‰JÈn…L………X…gÒ∫", #format: #integer, #default: EMPTY], #animemax: [#comment: "…A…j…≈«√Ò·ÍÓ", #format: #integer, #default: 1]]  return descriptionlistendon exitFrame me  set cst to the number of member animestart + animenum  set the memberNum of sprite sprnum to cst  set animenum to animenum + 1  if animenum = animemax then    set animenum to 0  end ifend